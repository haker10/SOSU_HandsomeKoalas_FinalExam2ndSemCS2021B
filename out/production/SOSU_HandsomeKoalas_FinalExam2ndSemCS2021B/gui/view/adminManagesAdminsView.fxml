                    <font>
                        <Font name="Arial Bold Italic" size="12.0" />
                    </font>
                </TextField>
                <TextField fx:id="usernameTxt" alignment="CENTER" layoutX="18.0" layoutY="184.0" prefHeight="25.0" prefWidth="218.0" promptText="Username">
                    <font>
                        <Font name="Arial Bold Italic" size="12.0" />
                    </font>
                </TextField>
                <PasswordField fx:id="passwordTxt" alignment="CENTER" layoutX="18.0" layoutY="215.0" prefHeight="25.0" prefWidth="218.0" promptText="Password">
                    <font>
                        <Font name="Arial Bold Italic" size="12.0" />
                    </font>
                </PasswordField>
                <ChoiceBox fx:id="schoolChoiceBox" layoutX="52.0" layoutY="124.0" prefWidth="150.0" />
                <Button layoutX="76.0" layoutY="246.0" mnemonicParsing="false" onAction="#createAdmin" prefHeight="25.0" prefWidth="103.0" text="Create" />
                <Label alignment="CENTER" layoutX="50.0" layoutY="50.0" prefHeight="46.0" prefWidth="158.0" text="Create">
                    <font>
                        <Font name="System Bold" size="36.0" />
                    </font>
                </Label>
                <ImageView fitHeight="38.0" fitWidth="46.0" layoutX="108.0" layoutY="86.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../images/arrowDownImage.png" />
                    </image>
                </ImageView>
                <Label alignment="CENTER" layoutX="50.0" layoutY="271.0" prefHeight="52.0" prefWidth="158.0" text="Delete">
                    <font>
                        <Font name="System Bold" size="36.0" />
                    </font>
                </Label>
                <ImageView fitHeight="38.0" fitWidth="46.0" layoutX="108.0" layoutY="307.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../images/arrowDownImage.png" />
                    </image>
                </ImageView>
                <Label fx:id="schoolLbl" alignment="CENTER" layoutX="95.0" layoutY="14.0" prefHeight="17.0" prefWidth="103.0" styleClass="schoolLbl">
                    <font>
                        <Font name="Arial Bold Italic" size="12.0" />
                    </font></Label>
