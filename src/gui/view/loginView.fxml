   <children>
      <TextField fx:id="usernameTxt" layoutX="15.0" layoutY="14.0" promptText="Username" />
      <PasswordField fx:id="passwordTxt" layoutX="15.0" layoutY="41.0" promptText="Password" />
      <Button fx:id="logInBtn" layoutX="65.0" layoutY="84.0" mnemonicParsing="false" onAction="#login" text="Log In" />
   </children>
</AnchorPane>
