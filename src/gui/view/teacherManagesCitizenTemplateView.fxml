<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="442.0" prefWidth="845.0" stylesheets="@css/teacher.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.TeacherManagesCitizenTemplateController">
    <children>
        <TableView fx:id="citizenTemplateTV" prefHeight="442.0" prefWidth="571.0">
            <columns>
                <TableColumn fx:id="citizenTemplateNameColumn" prefWidth="570.0" text="Name" />
            </columns>
        </TableView>
        <AnchorPane layoutX="571.0" prefHeight="442.0" prefWidth="274.0" styleClass="Stage">
            <children>
                <TextField fx:id="citizenTemplateNameTxt" layoutX="14.0" layoutY="62.0" />
                <Button fx:id="createBtn" layoutX="178.0" layoutY="60.0" mnemonicParsing="false" onAction="#onClickCreate" prefHeight="30.0" prefWidth="75.0" text="Create" />
                <Button fx:id="editBtn" layoutX="59.0" layoutY="193.0" mnemonicParsing="false" onAction="#onClickEdit" prefHeight="46.0" prefWidth="156.0">
               <graphic>
                  <Label alignment="CENTER" prefHeight="45.0" prefWidth="103.0" text="Edit">
                     <font>
                        <Font name="System Bold" size="30.0" />
                     </font>
                  </Label>
               </graphic></Button>
                <Button fx:id="deleteBtn" layoutX="59.0" layoutY="120.0" mnemonicParsing="false" onAction="#onClickDelete" prefHeight="46.0" prefWidth="156.0">
               <graphic>
                  <Label alignment="CENTER" prefHeight="46.0" prefWidth="110.0" text="Delete">
                     <font>
                        <Font name="System Bold" size="30.0" />
                     </font>
                  </Label>
               </graphic></Button>
                <Button fx:id="copyBtn" layoutX="59.0" layoutY="266.0" mnemonicParsing="false" onAction="#onClickCopy" prefHeight="46.0" prefWidth="156.0">
               <graphic>
                  <Label alignment="CENTER" prefHeight="31.0" prefWidth="110.0" text="Copy">
                     <font>
                        <Font name="System Bold" size="30.0" />
                     </font>
                  </Label>
               </graphic></Button>
                <Button fx:id="createCitizenBtn" layoutX="59.0" layoutY="331.0" mnemonicParsing="false" onAction="#onClickCreateCitizen" prefHeight="103.0" prefWidth="156.0">
               <graphic>
                  <Label alignment="CENTER" prefHeight="98.0" prefWidth="173.0" text="Create Citizen" textAlignment="CENTER" wrapText="true">
                     <font>
                        <Font name="System Bold" size="30.0" />
                     </font>
                  </Label>
               </graphic></Button>
            <Label alignment="CENTER" layoutX="1.0" layoutY="14.0" prefHeight="46.0" prefWidth="258.0" text="Create CitizenTemplate">
               <font>
                  <Font name="System Bold" size="21.0" />
               </font>
            </Label>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
